{
  "name": "minio-proto-api",
  "version": "1.1.1",
  "description": "",
  "main": "./lib",
  "scripts": {
    "compose-up": "docker-compose -f test/docker-compose.yml up -d",
    "compose-down": "docker-compose -f test/docker-compose.yml down -v",
    "lint": "belly-button --fix -I 'node_modules/**' -I 'doc/**'",
    "predev": "npm run graphdoc && npm run compose-up",
    "dev": "NODE_ENV=development node -r ./.env.js --inspect dev-server.js",
    "postdev": "npm run compose-down",
    "fake": "graphql-faker ./lib/schema.graphql -p 4000",
    "faker": "graphql-faker ./lib/schema.graphql",
    "graphdoc": "graphdoc -s ./lib/schema.graphql -o ./doc --force",
    "pretest": "npm run compose-up",
    "test": "npm run lint && node -r ./.env.js node_modules/.bin/lab -v -a code -t 96",
    "posttest": "npm run compose-down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "boom": "7.1.1",
    "cb-barrier": "1.0.2",
    "dockerode": "2.5.3",
    "graphi": "5.0.0",
    "graphql-tools": "2.11.0",
    "keypair": "1.0.1",
    "mysql": "2.15.0",
    "node-forge": "0.7.1",
    "reach": "1.0.0",
    "uuid": "3.1.0",
    "wreck": "14.0.2"
  },
  "devDependencies": {
    "@2fd/graphdoc": "2.x.x",
    "belly-button": "4.x.x",
    "code": "5.x.x",
    "graphql-faker": "1.x.x",
    "hapi": "17.x.x",
    "hapi-pino": "3.x.x",
    "inert": "5.x.x",
    "lab": "15.x.x",
    "minio-proto-auth": "1.x.x"
  }
}
